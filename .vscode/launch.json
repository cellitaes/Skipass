  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Skipass Service [4000]",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Skipass/bin/Debug/net6.0/Skipass.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Skipass",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:4000"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
